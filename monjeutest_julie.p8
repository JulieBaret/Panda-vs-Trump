function _init()
	p={spr=1,x=60,y=60,speed=4}
end

function _update60()
	if btn(➡️) then
 p.x+=1 p.spr+=1
 	if (p.spr>3) p.spr=1
 end
	if btn(⬅️) then
	p.x-=1 p.spr=17 p.spr+=1
		if (p.spr>19) p.spr=17
	end
	if (btn(⬆️)) p.y-=1
	if (btn(⬇️)) p.y+=1
end

function _draw()
	cls()
	map()
	spr(p.spr,p.x,p.y)
end
	

